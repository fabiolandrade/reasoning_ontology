<?xml version="1.0"?>
<Ontology xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.semanticweb.org/fabíolaandrade/ontologies/2024/7/untitled-ontology-85"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     ontologyIRI="http://www.semanticweb.org/fabíolaandrade/ontologies/2024/7/untitled-ontology-85">
    <Prefix name="" IRI="http://www.semanticweb.org/fabíolaandrade/ontologies/2024/7/untitled-ontology-85"/>
    <Prefix name="owl" IRI="http://www.w3.org/2002/07/owl#"/>
    <Prefix name="rdf" IRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <Prefix name="xml" IRI="http://www.w3.org/XML/1998/namespace"/>
    <Prefix name="xsd" IRI="http://www.w3.org/2001/XMLSchema#"/>
    <Prefix name="rdfs" IRI="http://www.w3.org/2000/01/rdf-schema#"/>

    <!-- Subclasses for OSM_Tags -->

    <!-- Example: Mapping "Edifício de Saúde" to OSM tags -->
    <Declaration>
        <Class IRI="#OSM_Tags/healthcare_hospital"/>
    </Declaration>
    <SubClassOf>
        <Class IRI="#OSM_Tags/healthcare_hospital"/>
        <Class IRI="#OSM_Tags"/>
    </SubClassOf>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#OSM_Tags/healthcare_hospital</IRI>
        <Literal xml:lang="en">healthcare=hospital</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:isDefinedBy"/>
        <IRI>#OSM_Tags/healthcare_hospital</IRI>
        <Literal xml:lang="en">Represents hospitals in the OSM tagging schema.</Literal>
    </AnnotationAssertion>
    <EquivalentClasses>
        <Class IRI="#Edif_Saude"/>
        <ObjectSomeValuesFrom>
            <ObjectProperty IRI="#hasOSMTag"/>
            <Class IRI="#OSM_Tags/healthcare_hospital"/>
        </ObjectSomeValuesFrom>
    </EquivalentClasses>

